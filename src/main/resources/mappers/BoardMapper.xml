<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.green.boardver2.board.BoardMapper">

    <insert id="postBoard">
        INSERT INTO board
        SET title = #{title}
        , contents = #{contents}
        , writer_id = #{writerId}
    </insert>


</mapper>